{
  "name": "webpagestudio/tasklist",
  "description": "MVC Task list mini",
  "minimum-stability": "stable",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Victor G."
    }
  ],
  "config": {
    "vendor-dir": "application/vendor"
  },
  "require": {
    "twig/twig": "^3.0",
    "twbs/bootstrap": "v4.5.2",
    "jasongrimes/paginator": "1.*",
    "ext-json": "*",
    "ext-pdo": "*",
    "php": ">=7.4.0"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "application/Core",
      "Exceptions\\": "application/Exceptions",
      "Controllers\\": "application/Controllers",
      "Install\\": "application/Install"
    }
  },
  "scripts": {
    "post-install-cmd": "@php application\\Install\\start.php",
    "post-update-cmd": [
      "@php application\\Install\\update.php"
    ]
  }
}