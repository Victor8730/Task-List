<nav aria-label="navigation example text-center">
        {#{{ pag | raw }}#}
    {% if paginator.numPages > 1 %}
        <ul class="pagination">
            {% if paginator.prevUrl %}
                <li class="page-item"><a href="{{ paginator.prevUrl }}" class="page-link">Previous</a></li>
            {% endif %}

            {% for page in paginator.pages %}
                {% if page.url %}
                    <li {{ page.isCurrent ? 'class="page-item active disabled"' : '' }}><a href="{{ page.url }}" class="page-link">{{ page.num }}</a></li>
                {% else %}
                    <li class="page-item disabled"><span>{{ page.num }}</span></li>
                {% endif %}
            {% endfor %}

            {% if paginator.nextUrl %}
                <li class="page-item"><a href="{{ paginator.nextUrl }}" class="page-link">Next</a></li>
            {% endif %}
        </ul>
    {% endif %}
</nav>